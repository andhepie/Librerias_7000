unit uUsuarioLog;

interface

uses Classes, System.JSON;

type
  TUsuarioLog = class(Tobject)
  private
    FListaEventos: TStringList;
  public
    constructor create;
    destructor destroy; override;

    procedure agregarEvento(ss: string);
    function obtenerEvento: Tjsonobject;
  published
  end;

implementation

end.
